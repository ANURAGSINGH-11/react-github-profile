{
  "name": "react-github-profile",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storybook-addons": "^1.6.1",
    "@kadira/storyshots": "^2.0.1",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "cross-env": "^3.1.3",
    "eslint": "^3.9.0",
    "eslint-config-kentcdodds": "^10.1.0",
    "eslint-import-resolver-webpack": "^0.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "npm-run-all": "^3.1.1",
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all --parallel build:*",
    "build:app": "react-scripts build",
    "build:storybook": "cross-env BABEL_ENV=production build-storybook --static-dir public --output-dir build/storybook",
    "lint": "eslint src/",
    "test": "npm-run-all --parallel test:*",
    "test:units": "react-scripts test --env=jsdom",
    "test:storyshots": "cross-env BABEL_ENV=test storyshots",
    "watch:test:storyshots": "npm run test:storyshots -- --watch --update-interactive",
    "validate": "cross-env CI=true npm-run-all --parallel test lint build",
    "storybook": "start-storybook --dont-track --port 9009 --static-dir public"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/react",
      "kentcdodds/jest",
      "kentcdodds/jsx-a11y",
      "kentcdodds/webpack"
    ]
  }
}
