{
  "name": "react-github-profile",
  "version": "0.1.0",
  "homepage": "https://react-github-profile.netlify.com/",
  "private": true,
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "@kadira/storyshots": "^2.0.1",
    "cross-env": "^3.1.3",
    "enzyme": "^2.5.1",
    "eslint": "^3.9.0",
    "eslint-config-kentcdodds": "^10.1.0",
    "npm-run-all": "^3.1.1",
    "react-addons-test-utils": "^15.3.2",
    "react-scripts": "0.7.0"
  },
  "dependencies": {
    "axios": "^0.15.2",
    "bootstrap": "^3.3.7",
    "glamor": "^2.17.14",
    "history": "^4.4.0",
    "husky": "^0.11.9",
    "match-sorter": "^1.6.0",
    "moment": "^2.15.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router": "4.0.0-alpha.5",
    "react-tooltip": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all build:*",
    "build:app": "react-scripts build",
    "build:storybook": "cross-env BABEL_ENV=production build-storybook --static-dir public --output-dir build/storybook",
    "precommit": "npm run validate -s",
    "lint": "eslint src/",
    "test": "npm-run-all --parallel test:*",
    "test:units": "cross-env CI=true react-scripts test --env=jsdom",
    "watch:test:units": "react-scripts test --env=jsdom",
    "test:storyshots": "cross-env BABEL_ENV=test storyshots",
    "watch:test:storyshots": "npm run test:storyshots -- --watch --update-interactive",
    "validate": "npm-run-all --parallel test lint build",
    "storybook": "start-storybook --dont-track --port 9009 --static-dir public"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/react",
      "kentcdodds/jest",
      "kentcdodds/jsx-a11y",
      "kentcdodds/webpack"
    ]
  }
}
