    "test": "react-scripts test --env=jsdom",
    "validate": "cross-env CI=true npm-run-all --parallel test lint build",
    "storybook": "start-storybook --dont-track --port 9009 --static-dir public"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/react",
      "kentcdodds/jest",
      "kentcdodds/jsx-a11y",
      "kentcdodds/webpack"
