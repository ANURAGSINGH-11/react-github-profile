{
  "name": "react-github-profile",
  "version": "0.1.0",
  "homepage": "https://react-github-profile.netlify.com/",
  "private": true,
  "dependencies": {
    "axios": "^0.15.3",
    "bootstrap": "^3.3.7",
    "glamor": "^2.20.12",
    "history": "^4.4.1",
    "husky": "^0.11.9",
    "match-sorter": "^1.6.0",
    "moment": "^2.17.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^4.0.0-alpha.6",
    "react-tooltip": "^3.2.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.33.1",
    "cross-env": "^3.1.3",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-kentcdodds": "^11.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1",
    "npm-run-all": "^3.1.2",
    "react-addons-test-utils": "^15.4.1",
    "react-scripts": "^0.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm-run-all build:*",
    "build:app": "react-scripts build",
    "build:storybook": "cross-env BABEL_ENV=production build-storybook --static-dir public --output-dir build/storybook",
    "precommit": "npm run validate -s",
